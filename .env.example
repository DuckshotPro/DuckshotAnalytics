# Database
DATABASE_URL=postgresql://username:password@localhost:5432/duckshots

# Session
SESSION_SECRET=replace_with_a_secure_random_string

# Stripe (optional for demo)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
